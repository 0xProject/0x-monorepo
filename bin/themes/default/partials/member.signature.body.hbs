{{> member.sources}}
{{> comment}}

{{#if children}}
    <h4>Parameters</h4>
    <ul>
        {{#each children}}
            <li>
                <h5>{{#compact}}
                    {{name}}{{#if isOptional}}?{{/if}}:&nbsp;{{#with type}}{{>type}}{{/with}}

                    {{#each flagsArray}}
                        &nbsp;<span class="tsd-flag ts-flag{{this}}">{{this}}</span>
                    {{/each}}
                {{/compact}}</h5>
                {{> comment}}

                {{#if children}}
                    {{> parameter}}
                {{/if}}
            </li>
        {{/each}}
    </ul>
{{/if}}

{{#if type}}
    <h4>Returns {{#with type}}{{>type}}{{/with}}</h4>
    {{#if comment.returns}}
        {{#markdown}}{{{comment.returns}}}{{/markdown}}
    {{/if}}
{{/if}}