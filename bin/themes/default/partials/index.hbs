<section class="tsd-member-index">
    <h2>Index</h2>
    {{#each groups}}
        <h3>{{title}}</h3>
        <ul class="tsd-member-index">
            {{#each children}}
                <li class="{{cssClasses}}"><a href="{{relativeURL url}}">{{name}}</a></li>
            {{/each}}
        </ul>
    {{/each}}
</section>