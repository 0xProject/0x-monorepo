<section class="tsd-member {{cssClasses}}">
    <a name="{{anchor}}" class="anchor"></a>
    {{#if signatures}}
        {{#each signatures}}
            {{> member.signature}}
        {{/each}}
    {{else}}
        {{> member.declaration}}
    {{/if}}

    {{#each groups}}
        {{#each children}}
            {{#unless hasOwnDocument}}
                {{> member}}
            {{/unless}}
        {{/each}}
    {{/each}}
</section>