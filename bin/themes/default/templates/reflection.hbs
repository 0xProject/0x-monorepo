{{#with model}}
    {{#if hasComment}}
        <section class="tsd-panel tsd-comment">
            {{> comment}}
        </section>
    {{/if}}
{{/with}}

{{#if model.typeHierarchy}}
    <section class="tsd-panel tsd-hierarchy">
        <h3>Hierarchy</h3>
        {{#with model.typeHierarchy}}{{> hierarchy}}{{/with}}
    </section>
{{/if}}

{{#with model}}
    {{> index}}
    {{> members}}
{{/with}}