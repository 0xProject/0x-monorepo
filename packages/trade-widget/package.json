        "html-webpack-plugin": "2.29.0",
        "jest": "20.0.4",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.8",
        "promise": "8.0.1",
        "raf": "3.4.0",
        "react": "^16.2.0",
        "react-dev-utils": "4.2.1",
        "react-dom": "^16.2.0",
        "source-map-loader": "^0.2.1",
        "style-loader": "0.19.0",
        "sw-precache-webpack-plugin": "0.11.4",
        "ts-jest": "^20.0.7",
        "ts-loader": "^2.3.7",
        "tsconfig-paths-webpack-plugin": "^2.0.0",
        "tslint": "^5.7.0",
        "tslint-react": "^3.2.0",
        "url-loader": "0.6.2",
        "webpack": "3.8.1",
        "webpack-dev-server": "2.9.4",
        "webpack-manifest-plugin": "1.3.2",
        "whatwg-fetch": "2.0.3"
    },
    "scripts": {
        "start": "node scripts/start.js",
        "build:webpack": "node scripts/build.js",
        "build": "tsc && yarn run copy_artifacts",
        "test": "node scripts/test.js --env=jsdom",
        "copy_artifacts": "copyfiles './src/artifacts/**/*' ./lib",
        "generate_contract_wrappers":
            "node ../abi-gen/lib/index.js --abis 'src/artifacts/@(Forwarder).json' --template ../contract_templates/contract.handlebars --partials '../contract_templates/partials/**/*.handlebars' --output src/contract_wrappers/generated",
        "update_contracts":
            "for i in ${npm_package_config_artifacts}; do copyfiles -u 3 ../contracts/artifacts/$i.json ../trade-widget/src/artifacts; done;",
        "styleguide": "styleguidist server",
        "compile":
            "node ../deployer/lib/src/cli.js compile --contracts ${npm_package_config_contracts} --contracts-dir src/contracts --artifacts-dir src/artifacts",
        "migrate": "yarn build && yarn compile && node ./lib/migrations/index.js",
        "styleguide:build": "styleguidist build"
    },
    "config": {
        "contracts": "Forwarder",
        "artifacts": "Forwarder"
    },
    "devDependencies": {
        "0x.js": "^0.33.6",
        "@0xproject/abi-gen": "^0.2.7",
        "@0xproject/base-contract": "^0.0.5",
        "@0xproject/deployer": "^0.3.2",
        "@0xproject/dev-utils": "^0.3.0",
        "@0xproject/subproviders": "^0.8.2",
        "@0xproject/tslint-config": "^0.4.9",
        "@0xproject/types": "^0.4.1",
        "@0xproject/utils": "^0.4.3",
        "@0xproject/web3-wrapper": "^0.3.1",
        "@types/lodash": "^4.14.104",
        "@types/node": "^9.4.6",
        "@types/prop-types": "^15.5.2",
        "@types/react": "^16.0.38",
        "@types/react-dom": "^16.0.4",
        "awesome-typescript-loader": "^3.4.1",
        "copyfiles": "^1.2.0",
        "ethers-contracts": "^2.2.1",
        "ethers-typescript-typings": "^0.0.4",
        "json-loader": "^0.5.7",
        "lodash": "^4.17.5",
        "prop-types": "^15.6.1",
        "react": "^16.2.0",
        "react-docgen-typescript": "^1.2.5",
        "react-redux": "^5.0.7",
        "react-scripts-ts": "^2.13.0",
        "react-styleguidist": "^6.2.7",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "tslint": "^5.9.1",
        "typescript": "^2.7.2",
        "web3": "^0.20.0",
        "web3-provider-engine": "^13.6.5",
        "web3-typescript-typings": "^0.10.0",
        "webpack": "^3.11.0"
    },
    "jest": {
        "collectCoverageFrom": ["src/**/*.{js,jsx,ts,tsx}"],
        "setupFiles": ["<rootDir>/config/polyfills.js"],
        "testMatch": ["<rootDir>/src/**/__tests__/**/*.ts?(x)", "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "mjs",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "web.js",
            "js",
            "web.jsx",
            "jsx",
            "json",
            "node"
        ],
        "globals": {
            "ts-jest": {
                "tsConfigFile": "/Users/jacob/projects/ethdev/0x/0x.js/packages/forwarder-widget/tsconfig.test.json"
            }
        }
    },
    "babel": {
        "presets": ["react-app"]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
