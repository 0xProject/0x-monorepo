{
  "name": "react-ts-starter-project",
  "version": "1.0.0",
  "description": "React starter project",
  "main": "lib/index.js",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack",
    "build": "tsc && yarn run copy_artifacts",
    "clean": "shx rm -f src/public/bundle*",
    "lint": "tslint --project . 'src/ts/**/*.ts' 'src/ts/**/*.tsx'",
    "dev": "webpack-dev-server --open",
    "generate_contract_wrappers": "node ../abi-gen/lib/index.js --abis 'src/ts/artifacts/@(Forwarder).json' --template ../contract_templates/contract.handlebars --partials '../contract_templates/partials/**/*.handlebars' --output src/contract_wrappers/generated",
    "update_contracts": "for i in ${npm_package_config_artifacts}; do copyfiles -u 3 ../contracts/artifacts/$i.json ../trade-widget/src/artifacts/; done;",
    "styleguide": "styleguidist server",
    "compile": "node ../deployer/lib/src/cli.js compile --contracts ${npm_package_config_contracts} --contracts-dir src/contracts --artifacts-dir src/artifacts",
    "migrate": "yarn build && yarn compile && node ./lib/migrations/index.js",
    "styleguide:build": "styleguidist build"
  },
  "config": {
    "contracts": "Forwarder",
    "artifacts": "Forwarder"
  },
  "author": "Jacob Evans",
  "license": "MIT",
  "devDependencies": {
    "0x.js": "^0.33.6",
    "@0xproject/abi-gen": "^0.2.7",
    "@0xproject/base-contract": "^0.0.3",
    "@0xproject/deployer": "^0.2.1",
    "@0xproject/dev-utils": "^0.3.0",
    "@0xproject/subproviders": "^0.8.2",
    "@0xproject/tslint-config": "^0.4.12",
    "@0xproject/types": "^0.4.1",
    "@0xproject/utils": "^0.4.1",
    "@0xproject/web3-wrapper": "^0.2.1",
    "@types/ethereumjs-util": "^5.1.0",
    "@types/lodash": "^4.14.86",
    "@types/material-ui": "0.18.0",
    "@types/node": "^8.0.53",
    "@types/react": "^15.0.15",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.0.4",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/yargs": "^11.0.0",
    "awesome-typescript-loader": "^3.4.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.9",
    "delay": "^2.0.0",
    "ethers-contracts": "^2.2.1",
    "ethers-typescript-typings": "^0.0.4",
    "json-loader": "^0.5.7",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "react": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "^2.13.0",
    "react-styleguidist": "^6.2.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "shx": "^0.2.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.2",
    "tslint": "^5.9.1",
    "typescript": "2.7.1",
    "web3": "^0.20.0",
    "web3-provider-engine": "^13.6.5",
    "web3-typescript-typings": "^0.10.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "bloomer": "^0.6.3",
    "bn.js": "^4.11.8",
    "bulma": "^0.6.2",
    "copyfiles": "^1.2.0",
    "lodash": "^4.17.4",
    "react-dev-utils": "4.2.1",
    "react-dom": "15.6.1",
    "react-router-dom": "^4.1.1",
    "react-router-hash-link": "^1.1.0",
    "react-tap-event-plugin": "^2.0.1"
  }
}
