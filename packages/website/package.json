        "material-ui": "0.17.4",
        "moment": "2.21.0",
        "query-string": "4.3.4",
        "react": "15.6.1",
        "react-copy-to-clipboard": "4.3.1",
        "react-document-title": "2.0.3",
        "react-dom": "15.6.1",
        "react-ga": "2.4.1",
        "react-redux": "5.0.7",
        "react-router-dom": "4.2.2",
