        "redux": "^3.6.0",
        "scroll-to-element": "^2.0.0",
        "semver-sort": "0.0.4",
        "thenby": "^1.2.3",
        "truffle-contract": "2.0.1",
        "tslint-config-0xproject": "^0.0.2",
        "web3": "^0.20.0",
        "web3-provider-engine": "^13.0.1",
        "whatwg-fetch": "^2.0.3",
        "xml-js": "^1.3.2"
    },
    "devDependencies": {
        "@types/accounting": "^0.4.1",
        "@types/dateformat": "^1.0.1",
        "@types/deep-equal": "^1.0.0",
        "@types/jsonschema": "^1.1.1",
