        "@types/redux": "^3.6.0",
        "awesome-typescript-loader": "^3.1.3",
        "copy-webpack-plugin": "^4.0.1",
        "copyfiles": "^1.2.0",
        "css-loader": "0.23.x",
        "exports-loader": "0.6.x",
