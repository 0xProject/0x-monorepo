{
    "name": "@0xproject/react-shared",
    "version": "0.2.3",
    "engines": {
        "node": ">=6.12"
    },
    "description": "0x shared react components",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "lint": "tslint --project .",
        "build": "tsc && copyfiles -u 2 './lib/monorepo_scripts/**/*' ./scripts",
