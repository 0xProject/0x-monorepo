{
    "name": "@0x/trading-client",
    "version": "0.0.1",
    "engines": {
        "node": ">=6.12"
    },
    "description": "Provides a convenient interface to execute transactions involving a Coordinator contract or other extension contracts",
    "main": "lib/src/index.js",
    "types": "lib/src/index.d.ts",
    "scripts": {
        "build": "tsc -b",
        "build:ci": "yarn build",
        "clean": "shx rm -rf lib test_temp",
        "lint": "tslint --format stylish --project .",
        "run_mocha": "mocha --require source-map-support/register --require make-promises-safe lib/test/**/*_test.js --exit",
        "test": "yarn run_mocha",
        "rebuild_and_test": "run-s clean build test",
        "test:coverage": "nyc npm run test --all && yarn coverage:report:lcov",
        "coverage:report:lcov": "nyc report --reporter=text-lcov > coverage/lcov.info",
        "test:circleci": "yarn test:coverage"
    },
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/0xProject/0x-monorepo.git"
    },
    "bugs": {
        "url": "https://github.com/0xProject/0x-monorepo/issues"
    },
    "homepage": "https://github.com/0xProject/0x-monorepo/packages/trading-client/README.md",
    "devDependencies": {
        "@0x/tslint-config": "^3.0.0",
        "@types/mocha": "^2.2.42",
        "@types/valid-url": "^1.0.2",
        "chai": "^4.0.1",
        "dirty-chai": "^2.0.1",
        "make-promises-safe": "^1.1.0",
        "mocha": "^4.1.0",
        "npm-run-all": "^4.1.2",
        "nyc": "^11.0.1",
        "shx": "^0.2.2",
        "tslint": "5.11.0",
        "typescript": "3.0.1"
    },
    "dependencies": {
        "@0x/abi-gen-wrappers": "^4.1.0",
        "@0x/assert": "^2.0.8",
        "@0x/contract-addresses": "^2.3.0",
        "@0x/contract-artifacts": "^1.4.0",
        "@0x/contract-wrappers": "^8.0.5",
        "@0x/json-schemas": "^3.0.8",
        "@0x/order-utils": "^7.1.1",
        "@0x/types": "^2.2.1",
        "@0x/typescript-typings": "^4.2.1",
        "@0x/utils": "^4.3.0",
        "@0x/web3-wrapper": "^6.0.4",
        "ethereum-types": "^2.1.1",
        "valid-url": "^1.0.9"
    },
    "publishConfig": {
        "access": "public"
    }
}
